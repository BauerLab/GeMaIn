  issue           = {7},
  keywords        = {Aged; Aged, 80 and over; Bone Density, physiology; Creatinine, blood; Female; Femur Neck; Glomerular Filtration Rate; Humans; Kidney Diseases, blood, epidemiology; Lumbar Vertebrae; Male; Middle Aged; Nutrition Surveys; Osteoporosis, blood, epidemiology; Republic of Korea, epidemiology; Sarcopenia, blood, epidemiology},
  nlm             = {PMC4514793},
  nlm-id          = {101285081},
