  issue           = {5},
  keywords        = {Animals; Bone Density; Bone Development; Bone Resorption, physiopathology; Calcification, Physiologic; Gene Deletion; Hindlimb; Mice; Mice, Inbred Strains; Osteoclasts, physiology; Osteogenesis, physiology; Osteopontin, deficiency, genetics, physiology; Restriction Mapping; Weight-Bearing},
